&lt;/span>&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">&lt;span class="w"> &lt;/span>&lt;span class="cp">#[wasm_bindgen(constructor)]&lt;/span>&lt;span class="w">
